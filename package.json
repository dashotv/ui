{
  "name": "dashotv",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@clerk/clerk-react": "4.26.6",
    "@clerk/themes": "1.7.8",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fontsource/roboto": "5.0.8",
    "@mui/icons-material": "5.14.14",
    "@mui/material": "5.14.14",
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@tanstack/react-query": "5.0.0",
    "@tanstack/react-query-devtools": "5.0.3",
    "@types/node": "20.8.7",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.29",
    "@vitejs/plugin-react": "4.1.0",
    "axios": "1.5.1",
    "dayjs": "1.11.10",
    "http-proxy-middleware": "2.0.6",
    "lint-staged": "15.0.2",
    "nats.ws": "1.18.0",
    "notistack": "3.0.1",
    "prettier": "3.0.3",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-helmet-async": "1.3.0",
    "react-hotkeys-hook": "4.4.1",
    "react-loader-spinner": "5.4.5",
    "react-router-dom": "6.17.0",
    "sanitize.css": "13.0.0",
    "sass": "1.69.4",
    "styled-components": "6.1.0",
    "stylelint": "15.11.0",
    "typescript": "5.2.2",
    "usehooks-ts": "2.9.1",
    "vite": "4.5.0",
    "vite-plugin-node-polyfills": "0.15.0",
    "vite-plugin-svgr": "4.1.0",
    "vite-tsconfig-paths": "4.2.1",
    "web-vitals": "3.5.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage --watch=false",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "prettify": "prettier --write",
    "docker:build": "docker build -t dashotv/ui .",
    "docker:run": "docker run -it --rm --name ui -p 3000:80 dashotv/ui",
    "docker:exec": "docker exec -it ui sh",
    "docker:push": "docker push dashotv/ui"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14.x"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "@vitest/coverage-v8": "0.34.6",
    "jsdom": "22.1.0",
    "vitest": "0.34.6"
  }
}
